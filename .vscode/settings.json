{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".lua",
                "cmd": "powershell ${workspaceFolder}/build-tools-windows.ps1 ${fileDirname}"
            }
        ]
    },
    "explorer.sortOrder": "default",
    "files.associations": {
        "**/*.lua": "pico8p8"
    },
    "files.autoSave": "off"
}
